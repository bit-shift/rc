# vim: set syntax=zsh:
# paths: options for PATH and its language-specific cousins
pathmunge $HOME/bin
pathmunge $HOME/.local/bin

pathmunge $HOME/android-sdk-linux/platform-tools
pathmunge $HOME/android-sdk-linux/tools
pathmunge $HOME/pharo2.0

export PYTHONSTARTUP=~/.pythonrc.py

unset GOROOT  # distro-provided Go packages have their own GOROOT hardcoded, so setting this would only fuck them up
export GOPATH=$HOME/.go
pathmunge $GOPATH/bin

export PATH
